<!-- Header Section -->
<section class="header-section">
	<div class="container">
		<div class="row">
			<div class="col-lg-12">
				<img src="img/catersquad-white.png">
				<h1>Create your event</h1>
			</div>
		</div>
	</div>
</section>

<div class="page-wrap">
<!-- Selection Section -->
<section class="selection-section">
	<div class="container">
		<div id="eventInformation">
			<div class="row">
				<div class="col-lg-12">
					<div class="file_select">
						<div id="drop_zone">
							Drop .ics file here
							<br />
							or
							<br />
							<input type="button" class="btn btn-info btn-sm" id="upload" value="select .ics file">
							<input id="upload_file" type="file">
						</div>
						<output id="list"></output>
					</div>
				</div>
			</div>
			<br>
			<div class="row">
				<div class="col-md-4">
					<div class="form-group">
						<label for="title">Title</label>
						<input type="text" class="form-control" id="title" placeholder="Interesting meeting!" required>
					</div>
				</div>
				<div class="col-md-4">
					<div class="form-group">
						<label for="datetimepickerstart">Start</label>
						<div class='input-group date' id='datetimepickerstart'>
							<input type='text' class="form-control" placeholder="01/01/2015 10:00 AM" reqiured id="timestart"/>
						<span class="input-group-addon">
							<span class="glyphicon glyphicon-calendar"></span>
						</span>
						</div>
					</div>
				</div>
				<div class="col-md-4">
					<div class="form-group">
						<label for="datetimepickerend">End</label>
						<div class='input-group date' id='datetimepickerend'>
							<input type='text' class="form-control" placeholder="01/01/2015 11:00 AM (optional)" id="timeend"/>
						<span class="input-group-addon">
							<span class="glyphicon glyphicon-calendar"></span>
						</span>
						</div>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="col-lg-12">
					<div class="form-group">
						<label for="description">Description:</label>
						<textarea class="form-control" rows="5" id="description" placeholder="Description of your event (optional)"></textarea>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="col-lg-12">
					<div class="form-group">
						<label for="location">Location:</label>
						<input placeholder="Dalgas Avenue 10, 8000 Aarhus" type="text" class="form-control" id="location" required>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="col-lg-12">
					<div class="form-group">
						<label for="participants">Participants:</label>
						<input placeholder="mail@me.com" type="text" class="form-control" id="participants" required>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="col-sm-4">
					<div class="form-group">
						<label class="control-label" for="budget">Budget:</label>
						<div class="input-group">
							<div class="input-group-addon"><span class="glyphicon glyphicon-usd"></span></div>
							<input type="number" class="form-control" id="budget" placeholder="0">
						</div>
					</div>
				</div>
				<div class="col-sm-4">
					<div class="form-group">
						<label class="control-label" for="prperson">Pr. person:</label>
						<div class="input-group">
							<div class="input-group-addon"><span class="glyphicon glyphicon-usd"></span></div>
							<input type="number" class="form-control" id="prperson" placeholder="0">
						</div>
					</div>
				</div>
				<div class="col-sm-4">
					<div class="form-group">
						<label class="control-label" for="numberparticipants">Participants:</label>
						<div class="input-group">
							<div class="input-group-addon"><span class="glyphicon glyphicon-user"></span></div>
							<input type="number" class="form-control" id="numberparticipants" placeholder="0" disabled>
						</div>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="col-lg-12">
					<button onclick="chooserestaurant()" class="btn-xl">Choose Restaurant</button>
				</div>
			</div>
		</div>


		<div id="restaurantInformation" hidden>
			<ul class="list-group">
				<li>
					<div class="panel panel-default restaurant-card picked-card">
						<div class="panel-heading">
							<p>Heading</p>
						</div>
						<div class="panel-body">
							<div class="panel-info">
								<p>info</p>
							</div>

						</div>
						<div class="panel-footer">
							<p>footer</p>
						</div>
					</div>
				</li>
				<li>
					<div class="panel panel-default restaurant-card">
						<div class="panel-heading">
							<p>Heading</p>
						</div>
						<div class="panel-body">
							<div class="panel-info">
								<p>info</p>
							</div>

						</div>
						<div class="panel-footer">
							<p>footer</p>
						</div>
					</div>
				</li>
				<li>
					<div class="panel panel-default restaurant-card">
						<div class="panel-heading">
							<p>Heading</p>
						</div>
						<div class="panel-body">
							<div class="panel-info">
								<p>info</p>
							</div>
						</div>
						<div class="panel-footer">
							<p>footer</p>
						</div>
					</div>
				</li>
				<li>
					<div class="panel panel-default restaurant-card">
						<div class="panel-heading">
							<p>Heading</p>
						</div>
						<div class="panel-body">
							<div class="panel-info">
								<p>info</p>
							</div>

						</div>
						<div class="panel-footer">
							<p>footer</p>
						</div>
					</div>
				</li>
				<li>
					<div class="panel panel-default restaurant-card">
						<div class="panel-heading">
							<p>Heading</p>
						</div>
						<div class="panel-body">
							<div class="panel-info">
								<p>info</p>
							</div>

						</div>
						<div class="panel-footer">
							<p>footer</p>
						</div>
					</div>
				</li>
			</ul>

			<div class="row">
				<div class="col-lg-12">
					<button onclick="submit()" class="btn-xl">Create Event</button>
				</div>
			</div>
		</div>

		<div id="successInformation" hidden>
			<div class="row">
				<h1>Success</h1>
				<p>Be ready for some delicious meals</p>
				<p>Pay here!</p>
			</div>
		  	<div class="row">
				<div class="col-lg-12">
			  		<button onclick="window.location.reload()" class="btn-xl">Create another event</button>
				</div>
		  	</div>
		</div>
	</div>
</section>

</div>
